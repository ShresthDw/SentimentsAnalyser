{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "templates/index.html",
      "use": "@vercel/static-build",
      "config": { "zeroConfig": true }
    }
  ],
  "routes": [
    {
      "src": "/predict",
      "dest": "api/app.py"
    },
    {
      "src": "/",
      "dest": "templates/index.html"
    }
  ]
}